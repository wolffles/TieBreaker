  "bugs": {
    "url": "https://github.com/wolffles/TieBreaker/issues"
  },
  "homepage": "https://github.com/wolffles/TieBreaker#readme",
  "dependencies": {
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "nodemon": "^3.0.4",
    "vite": "^6.2.6"
  },
